\n          Search Age:\n          <input className=\"search-age\" placeholder=\"Min\" onChange={e => setMinAge(e.target.value)} />\n          To\n          <input className=\"search-age\" placeholder=\"Max\" onChange={e => setMaxAge(e.target.value)} />\n        </div>\n        <input className=\"search\" placeholder=\"Search Booking name ...\" onChange={e => searchName(e)} />\n      </div>\n      <table className=\"fixed-header\">\n        <thead>\n          <tr>\n            <th>Room Number</th>\n            <th>Booking name</th>\n            <th>Age</th>\n            <th>Availability</th>\n          </tr>\n        </thead>\n        <tbody>\n          {\n            bookingState.map(b => {\n              return (\n                <tr>\n                  <td width=\"15%\">{b.roomNumber}</td>\n                  <td width=\"35%\">{b.name}</td>\n                  <td width=\"10%\">{b.age}</td>\n                  <td width=\"20%\" className={b.isBooking ? \"unavailable\" : \"available\"} />\n                </tr>\n              )\n            })\n          }\n        </tbody>\n      </table>\n    </div >\n  )\n}\n\nconst mapStateToProps = function (state) {\n  return {\n    booking: state.manageBooking.booking\n  }\n}\n\nconst HomeWithConnect = connect(mapStateToProps)(Home)\nexport default HomeWithConnect\n","/Users/awopal/Documents/test/hotelmanage/src/common/fieldInput.js",[],"/Users/awopal/Documents/test/hotelmanage/src/reducers/manageBooking.js",[],"/Users/awopal/Documents/test/hotelmanage/src/index.js",[],"/Users/awopal/Documents/test/hotelmanage/src/common/sidebar.js",[],"/Users/awopal/Documents/test/hotelmanage/src/reducers/index.js",[],"/Users/awopal/Documents/test/hotelmanage/src/page/keycard/index.js",[],"/Users/awopal/Documents/test/hotelmanage/src/page/checkout/index.js",[],"/Users/awopal/Documents/test/hotelmanage/src/page/register/index.js",[],{"ruleId":"51","replacedBy":"52"},{"ruleId":"53","replacedBy":"54"},{"ruleId":"55","severity":1,"message":"56","